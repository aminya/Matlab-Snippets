                {"name":"SelectedVariableNames", "kind":"namevalue", "type":[["cell"], ["char"], ["string"]]},
                {"name":"ReadSize", "kind":"namevalue", "type":[["numeric"],["scalar"], ["string", "choices={'rowgroup', 'file'}"]], "default" : "'rowgroup'"},
                {"name":"OutputType", "kind": "namevalue", "type": ["string", "choices={'auto','timetable'}"]},
