                {"name":"ReadSize", "kind":"namevalue", "type":[["numeric"],["scalar"], ["string", "choices={'rowgroup', 'file'}"]], "default" : "'rowgroup'"},
                {"name":"OutputType", "kind": "namevalue", "type": ["string", "choices={'auto','table','timetable'}"], "default":"'table'"},
                {"name":"PreserveVariableNames", "kind":"namevalue", "type":["logical", "scalar"], "default": "false"}
