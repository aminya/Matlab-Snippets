        body: '''${1:h} = plotm(${1:[lat lon]}, ${2:optional_linetype}, ${3:opts})'''
        description: '''h = plotm([lat lon], optional_linetype, opts)'''


    "zdatam":
        prefix: "zdatam"
        body: '''zdatam(${1:hndl}, ${2:optional_zdata})'''
        description: '''zdatam(hndl, optional_zdata)'''


    "northarrow":
        prefix: "northarrow"
        body: '''northarrow('latitude', ${1:value}, 'longitude', ${2:value}, 'facecolor', ${3:value}, 'edgecolor', ${4:value}, 'linewidth', ${5:value}, 'scaleratio', ${6:value})'''
        description: '''northarrow('latitude', value, 'longitude', value, 'facecolor', value, 'edgecolor', value, 'linewidth', value, 'scaleratio', value)'''


    "clipdata":
        prefix: "clipdata"
        body: '''[${1:lat}, ${2:long}, ${3:splitpts}] = clipdata(${1:lat}, ${2:long}, ${3:object})'''
        description: '''[lat, long, splitpts] = clipdata(lat, long, object)'''

